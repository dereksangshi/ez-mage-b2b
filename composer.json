{
    "name": "ez/mage-b2b",
    "type": "library",
    "description": "MageB2B",
    "keywords": ["magento", "b2b"],
    "homepage": "https://bitbucket.org/dereksangshi2000/ez-mage-b2b",
    "license": "MIT",
    "authors": [
        {"name": "Derek Li", "email": "dereksangshi2000@gmail.com.au"}
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/console": "~2.0",
        "symfony/dependency-injection": "2.7.*",
        "doctrine/orm": "2.5.*",
        "doctrine/mongodb-odm": "1.0.*",
        "ez/data-structure": "1.*",
        "ez/util": "1.*",
        "ez/mage-cli": "1.*",
        "ez/service": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://satis.internal.balancenet.com.au"
        }
    ],
    "autoload": {
        "psr-0": { "Ez\\MageB2B\\": "src/" }
    },
    "archive": {
        "exclude": ["!vendor", "tests", "*phpunit.xml", ".travis.yml", "build.xml", "build.properties", "composer.phar"]
    }
}
